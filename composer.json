{
  "name": "tobiaslau/geo-timezone",
  "homepage": "http://www.t-lau.de",
  "description": "PHP library for timezone calculation from a particular location and date.",
  "repositories": [
    {
      "type": "git",
      "url": "https://github.com/phayes/geoPHP",
      "autoload": {
        "files": [
          "vendor/phayes/geophp/geoPHP.inc"
        ]
      }
    }
  ],
  "require": {
    "phayes/geoPHP": "1.2.*",
    "php": "^7.2",
    "ext-dom": ">=7.2",
    "ext-mbstring": ">=7.2",
    "ext-zip": "*",
    "ext-xml":">=7.2",
    "phpunit/phpunit": "^7.0",
    "guzzle/guzzle": "5.3.0"
  },
  "autoload": {
    "psr-4": {
      "GeoTimeZone\\": "src/GeoTimeZone/",
      "Tests\\": "tests/"
    }
  },
  "config": {
    "vendor-dir": "vendor"
  }
}
